<?xml version="1.0" encoding="utf-8"?>
<com.dimelo.dimelosdk.utilities.SquareRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/DimeloRowGalleryTheme"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        />

    <View
        android:background="@drawable/dimelo_gallery_image_selector"
        android:id="@+id/image_clickable_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</com.dimelo.dimelosdk.utilities.SquareRelativeLayout>

